            <!-- Table Start -->
            
                    
                    <div class="col-12">
                        <div class="bg-black rounded h-100 p-4">
                           
                            <a href="/admin/addcoupon" class="btn btn-success mb-3" role="button">ADD NEW COUPON +</a>
                             <h6 class="mb-4">COUPONS LIST</h6>
                             
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th scope="col">SNo.</th>
                                            <th scope="col">Name</th>
                                            <th scope="col">Expiry Date</th>
                                            <th scope="col">Discount</th>
                                            <th scope="col">Max Discount upto</th>
                                            <th scope="col">Discount Valid From ₹</th>
                                            <th scope="col">Description</th>
                                            <th scope="col">Options</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        
                                        {{#each coupons}}
                                        <tr>
                                            <th class="create_index"></th>
                                            <td>{{this.couponCode}}</td>
                                             <td>{{this.date}}</td>
                                            <td>{{this.discount}}%</td>
                                            <td>₹{{this.max}}</td>
                                            <td>₹{{this.min}}</td>
                                            <td>{{this.description}}</td>
                                            
                                             <td>
                                             <a href="/admin/delete-coupon/{{this._id}}" class="ms-3" title="Delete Coupon" role="button"><span class="bi bi-trash-fill fa-2x ms-3"></span></a></td>
                                             
                                        </tr>  

                                        {{/each}}
                                    </tbody>
                                </table>

                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
