                    <div class="col-12">
                        <div class="bg-black rounded h-100 p-4">
                            <h6 class="mb-4">ORDER LIST</h6>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th>SNo.</th>
                                            <th scope="col">Name</th>
                                            <th scope="col">Date</th>
                                            <th scope="col">Mobile</th>
                                            <th scope="col">item</th>
                                            <th scope="col">Payment Method</th>
                                            <th scope="col">Total amount(â‚¹)</th>
                                            <th scope="col">Status</th>
                                            
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each order}}
                                        <tr>
                                            <th class="create_index"></th>
                                            <td class="text-warning">{{this.name}}</td>
                                            <td>{{this.date}}</td>
                                            <td style="color: yellow;">{{this.mobile}}</td>
                                            <td>{{this.item}}</td>
                                            <td>{{this.paymentMathod}}</td>
                                            <td>{{this.frelloPrice}}</td>
                                            <td>
                                                <select onchange="changeStatus('{{this._id}}','{{this.item}}','{{this.frelloPrice}}','{{this.userId}}','{{this.paymentMathod}}')" name="orderstatus" id="{{this.item}}">
                                                    <option selected value="{{this.status}}" class="text-success">{{this.status}}</option>

                                                    {{#isEqual this.status 'Placed'}}
                                                    
                                                    <option value="Despatched">Despatched</option>
                                                    <option value="Shipped">Shipped</option>
                                                    <option value="Shipped">Out for Delivery</option>
                                                    <option value="Delivered">Delivered</option>
                                                    {{/isEqual}}

                                                    {{#isEqual this.status 'Despatched'}}
                            
                                                    <option value="Shipped">Shipped</option>
                                                    <option value="Shipped">Out for Delivery</option>
                                                    <option value="Delivered">Delivered</option>
                                                    {{/isEqual}}

                                                    {{#isEqual this.status 'Shipped'}}
                                                   
                                                    <option value="Shipped">Out for Delivery</option>
                                                    <option value="Delivered">Delivered</option>
                                                    {{/isEqual}}

                                                    {{#isEqual this.status 'Out for Delivery'}}
                                                   
                                                    <option value="Delivered">Delivered</option>
                                                    {{/isEqual}}

                                                     {{#isEqual this.status 'Return Requested'}}
                                                   
                                                    <option value="Return Completed">Return Completed</option>
                                                    {{/isEqual}}

                                                    

                                                </select>
                                            </td>
                                            
                                             
                                        </tr>                                                   
                                        {{/each}}


                                    
                                    </tbody>
                                </table>

